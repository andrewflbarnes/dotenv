[alias]
	addu = add -u
	ams = am -3 --signoff *patch
	bvv = branch -vv
	del = branch -D
	this = rev-parse --abbrev-ref HEAD
	co = checkout
	cob = checkout -b
	aclone = "!f() { git clone git@github.com:andrewflbarnes/${1} ${@:2}; }; f"
	commita = commit --amend
	diffc = diff --cached
	diffl = "!f() { git diff HEAD~ $(git rev-parse --abbrev-ref HEAD); }; f"
	diffh = diff HEAD~
	diffmb = "!f() { git diff $(git merge-base $1 $2) ${@:2}; }; f"
	adog = log --all --decorate --oneline --graph
	dog = log --decorate --oneline --graph
	l1 = log --decorate -1
	mergec = merge --continue
	rebasec = rebase --continue
	ru = remote update
	rv = remote -v
	st = status --short --branch
[credential]
	helper = osxkeychain
[pull]
    rebase = false
[user]
	email = andrewflbarnes@hotmail.co.uk
	name = andrewflbarnes
